<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.myapplication.vmenu"
    android:background="@drawable/bg">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Vaccines"
        android:textSize="30dp"
        android:paddingTop="30dp"
        android:id="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        />

    <Button
        android:background="@color/material_deep_teal_500"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Hep  B"
        android:id="@+id/button"
        android:layout_marginTop="60dp"
        android:layout_below="@+id/textView6"
        android:onClick="hepbint"
        android:clickable="true"
        android:contextClickable="true" />

    <Button
        android:background="@color/material_deep_teal_500"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="DPT"
        android:id="@+id/button2"
        android:onClick="dptint"
        android:clickable="true"
        android:contextClickable="true"
        android:layout_alignTop="@+id/button"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:background="@color/abc_search_url_text_normal"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Haemophilus"
        android:id="@+id/button3"
        android:layout_below="@+id/button"
        android:layout_alignStart="@+id/button"
        android:onClick="haeint"
        android:layout_alignLeft="@+id/button"
        android:clickable="true"
        android:contextClickable="true" />

    <Button
        android:background="@color/abc_search_url_text_normal"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Polio"
        android:id="@+id/button4"
        android:onClick="polint"
        android:clickable="true"
        android:contextClickable="true"
        android:layout_below="@+id/button2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:background="@color/material_deep_teal_500"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Rota"
        android:id="@+id/button5"
        android:layout_below="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:onClick="rotaint"
        android:layout_alignLeft="@+id/button3"
        android:clickable="true"
        android:contextClickable="true" />

    <Button
        android:background="@color/material_deep_teal_500"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Influenza"
        android:id="@+id/button6"
        android:layout_alignTop="@+id/button5"
        android:layout_alignStart="@+id/button4"
        android:onClick="inflint"
        android:layout_alignLeft="@+id/button4"
        android:clickable="true"
        android:contextClickable="true" />

    <Button
        android:background="@color/abc_search_url_text_normal"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="MMR"
        android:id="@+id/button7"
        android:layout_below="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:onClick="mmrint"
        android:layout_alignLeft="@+id/button5"
        android:clickable="true"
        android:contextClickable="true" />

    <Button
        android:background="@color/abc_search_url_text_normal"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="pneumococcal"
        android:textSize="13dp"
        android:id="@+id/button8"
        android:layout_alignTop="@+id/button7"
        android:layout_alignStart="@+id/button6"
        android:onClick="pneint"
        android:layout_alignLeft="@+id/button6"
        android:clickable="true"
        android:contextClickable="true" />

    <Button
        android:background="@color/material_deep_teal_500"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Vericella"
        android:id="@+id/button9"
        android:layout_below="@+id/button7"
        android:layout_alignStart="@+id/button7"
        android:onClick="variint"
        android:layout_alignLeft="@+id/button7"
        android:clickable="true"
        android:contextClickable="true" />

    <Button
        android:background="@color/material_deep_teal_500"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Hep  A"
        android:id="@+id/button10"
        android:layout_below="@+id/button8"
        android:layout_alignStart="@+id/button8"
        android:onClick="hepaint"
        android:layout_alignLeft="@+id/button8"
        android:clickable="true"
        android:contextClickable="true" />
</RelativeLayout>
